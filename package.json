  },
  "scripts": {
    "build": "vite build && tsc",
    "prepublish": "npm run build"
  },
  "keywords": [
    "extract",
    "extract-valid",
    "object",
    "scheme"
  ],
  "author": "vinsurs <2407232109@qq.com>",
