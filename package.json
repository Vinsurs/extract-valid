{
  "name": "extract-valid",
  "version": "1.1.0",
  "description": "A library for extracting valid fields from object scheme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinsurs/extract-valid.git"
  },
  "main": "dist/extract-valid.umd.js",
  "module": "dist/extract-valid.es.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/extract-valid.umd.js",
